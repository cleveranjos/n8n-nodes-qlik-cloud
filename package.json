{
  "name": "n8n-nodes-qlik-cloud",
  "version": "1.0.18",
  "description": "n8n node for Qlik Cloud REST APIs integration",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "tsc && npm run copy-assets",
    "copy-assets": "cp -r src/icons/* dist/icons || true",
    "dev": "tsc --watch",
    "lint": "eslint src/",
    "test": "jest",
    "publish": "npm run build && npm version patch && npm publish",
    "prepublishOnly": "npm run build"
  },
  "n8n": {
    "nodes": [
      "dist/nodes/QlikCloud/QlikCloud.node.js"
    ],
    "credentials": [
      "dist/credentials/QlikCloudApi.credentials.js",
      "dist/credentials/QlikCloudOAuth2Api.credentials.js"
    ]
  },
  "keywords": [
    "n8n",
    "qlik",
    "qlik-cloud",
    "api",
    "automation"
  ],
  "author": "Clever Anjos (cleveranjos@gmail.com)",
  "license": "MIT",
  "devDependencies": {
    "@types/node": "^18.0.0",
    "@typescript-eslint/eslint-plugin": "^5.0.0",
    "@typescript-eslint/parser": "^5.0.0",
    "eslint": "^8.0.0",
    "jest": "^29.0.0",
    "typescript": "^5.0.0"
  },
  "dependencies": {
    "n8n-core": "*",
    "n8n-workflow": "*",
    "axios": "^1.4.0"
  }
}
